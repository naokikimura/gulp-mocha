{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "interface-name": [
      true,
      "never-prefix"
    ],
    "quotemark": [
      true,
      "single"
    ],
    "arrow-parens": [
      true,
      "ban-single-arg-parens"
    ]
  },
  "rulesDirectory": []
}
